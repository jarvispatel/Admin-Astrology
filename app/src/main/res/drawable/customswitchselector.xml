<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <gradient android:angle="270" android:endColor="#EEEFF1" android:startColor="#EEEFF1" />
            <corners android:radius="12.5dp" />
            <size android:width="22dp" android:height="22dp" />
            <stroke android:width="3dp" android:color="@color/navHeaderColor" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <gradient android:angle="270" android:endColor="#85EEEFF1" android:startColor="#85EEEFF1" />
            <corners android:radius="12.5dp" />
            <size android:width="22dp" android:height="22dp" />
            <stroke android:width="3dp" android:color="#0000ffff" />
        </shape>
    </item>
</selector>